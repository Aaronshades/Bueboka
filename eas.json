{
  "cli": {
    "version": ">= 3.9.3",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "base": {
      "android": {
        "image": "default",
        "env": {
          "PLATFORM": "android"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        }
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "extends": "base",
      "ios": {
        "channel": "main",
        "resourceClass": "m-medium"
      },
      "android": {
        "channel": "main"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed",
        "serviceAccountKeyPath": "${process.env.GOOGLE_SERVICE_ACCOUNT_PATH}"
      },
      "ios": {
        "ascAppId": "6448108838"
      }
    }
  }
}
